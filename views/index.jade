				//- | &nbsp;&nbsp; 02-33661573
			div.vcenter.col-md-4.col-xs-12
				img(src='seed/images/footer-logo-02.png' style='padding: 0 5% 5% 0;')
				img(src='seed/images/footer-logo-03.png' style='padding: 0 5% 5% 0;')
				img(src='seed/images/footer-logo-04.png' style='padding: 0 5% 5% 0;')




